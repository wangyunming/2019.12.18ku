/*下拉菜单*/
.menubox {
	position: absolute;
	z-index: 99999;
	border-left: 1px solid #eeeeee;
	width: 199px;
	height: 300px;
	top: 30px;
	left: 0px; /*display:none */;
}

.menubox-s {
	position: absolute;
	width: 203px;
	z-index: 99988;
	height: 303px;
	background: url(../images/menu_s.png) right bottom;
}

.menubox a {
	display: block;
}

.menubox a.bbn {
	border: none;
}

.menubox a.bbn {
	border-bottom: none;
}

.dlleftmenu {
	position: absolute;
	padding: 2px 0px 0px 0px;
	z-index: 99990;
}

.dlleftmenu .navicon {
	float: left;
	margin: 3px 20px 0px 30px;
}

.dlleftmenu dd {
	width: 198px;
	border-left: 1px solid #ffffff;
	border-top: 1px solid #ffffff;
	border-bottom: 1px solid #f2f2f2;
	line-height: 31px;
	height: 31px;
	float: left;
	position: relative;
	z-index: 10000;
}

.dlleftmenu dd a.pname {
	display: block;
	float: left;
	color: #333333;
	font-weight: bold;
	padding: 0px;
	height: 31px;
	line-height: 28px;
}

.dlleftmenu dd a.p1 {
	background-position: 20px -0px;
}

.dlleftmenu dd.bbn {
	border-bottom: none;
}

.leftmenu-show {
	display: none;
	position: absolute;
	z-index: 9;
	top: 0px;
	width: 240px;
	background: #ffffff;
	border: 1px solid #b50000;
	padding: 10px 20px;
}

.leftmenu-show p span {
	display: block;
	float: left;
}

.leftmenu-showt {
	padding-bottom: 5px;
	display: none;
}

.leftmenu-showm {
	height: 10px;
	margin-top: 4px;
	border-top: 1px solid #F2F2F2;
}

.leftmenu-showd {
	display: none;
}

.leftmenu-show .tit {
	font-weight: bold;
	height: 14px;
	line-height: 14px;
}

.leftmenu-show .con {
	margin: 2px 0px 2px 0px;
	display: block;
	line-height: 18px;
}

.leftmenu-show .con span {
	display: inline-block;
	height: 18px;
	color: #999;
	margin: 4px 0px 0px 0px;
}

.leftmenu-show .con span.spr {
	margin: 0px 5px;
}

.leftmenu-show .con span a {
	height: 16px;
	padding: 0px;
	margin: 0;
	font-weight: normal;
	color: #666;
}

.leftmenu-show .con span a:hover {
	color: #b50000;
}

.dlleftmenu dd:hover a.pname {
	margin: -1px 0px 0px -1px;
	width: 199px;
	border-left: 1px solid #b50000;
	border-bottom: 1px solid #b50000;
	border-top: 1px solid #b50000;
	height: 31px;
	background-color: #fff;
	position: absolute;
	z-index: 20;
}

.dlleftmenu dd:hover .leftmenu-show {
	position: absolute;
	top: -1px;
	left: 198px;
	background: #ffffff;
}

