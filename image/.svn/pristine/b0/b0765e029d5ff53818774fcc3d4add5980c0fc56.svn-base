form {position:relative;}
form .error{ float:left; font-size: 13px;}
form input[type='text'].error, form input[type='password'].error{border:1px solid #c00;}
form label.error, form span.error{padding: 2px 3px; color:#c00;}

input.error[type="text"], input.error[type="password"], input.error[type="number"], input.error[type="email"], input.error[type="url"], textarea.error, select.error{border:1px solid #c00;}
input.invalid[type="text"], input.invalid[type="password"], input.invalid[type="number"], input.invalid[type="email"], input.invalid[type="url"], textarea.invalid, select.invalid{border:1px solid #c00!important;}
input[type='text'].inactive,input[type='number'].inactive,
input[type='email'].inactive,input[type='url'].inactive,
textarea.inactive,select.inactive,option.inactive{color: #999;font-style: italic;font-size:12px;}

